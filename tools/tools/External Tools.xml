<toolSet name="External Tools">
  <tool name="Update remote" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="ssh" />
      <option name="PARAMETERS" value="dev '/home/hh/update_env.sh'" />
    </exec>
  </tool>
  <tool name="Clean" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/clean" />
    </exec>
  </tool>
  <tool name="Watch" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/watch" />
    </exec>
  </tool>
  <tool name="Before Debug" showInMainMenu="false" showInProject="false" useConsole="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/before_debug" />
    </exec>
  </tool>
  <tool name="Tail service log" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/log" />
    </exec>
  </tool>
  <tool name="Switch to sources" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/switch from-sources" />
    </exec>
  </tool>
  <tool name="Switch to package" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/switch from-pkg" />
    </exec>
  </tool>
  <tool name="Restart" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
    <exec>
      <option name="COMMAND" value="sh" />
      <option name="PARAMETERS" value="-c .idea/hhTools/restart" />
    </exec>
  </tool>
</toolSet>