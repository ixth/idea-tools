      <option name="COMMAND" value="ssh" />
      <option name="PARAMETERS" value="dev '/home/hh/update_env.sh'" />
    </exec>
  </tool>
  <tool name="Clean" showInMainMenu="true" showInProject="true" useConsole="true" synchronizeAfterRun="false">
